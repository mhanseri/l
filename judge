   // 判断答案是否正确
    if (userAnswer == correctAnswer) {
       cout << "Correct! The flase is true " << corrcetAnswer << "." << endl;
    } else {
        cout << "Wrong!get out the door " << corrcetAnswer << "." << endl;
    }
